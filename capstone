  #include <stdio.h>
#include <stdlib.h>
#include <time.h>

#define SIZE 1000

void circularScan(int *array, int size) {
    int i, j, temp;
    for (i = 0; i < size; i++) {
        for (j = 0; j < size - 1; j++) {
            if (array[j] > array[j + 1]) {
                temp = array[j];
                array[j] = array[j + 1];
                array[j + 1] = temp;
            }
        }
    }
}

void performanceAnalysis() {
    int *array = (int *)malloc(SIZE * sizeof(int));
    for (int i = 0; i < SIZE; i++) {
        array[i] = rand() % 1000;
    }

    clock_t start = clock();
    circularScan(array, SIZE);
    clock_t end = clock();

    double time_spent = (double)(end - start) / CLOCKS_PER_SEC;
    printf("Time taken for circular scan: %f seconds\n", time_spent);

    free(array);
}

int main() {
    performanceAnalysis();
    return 0;
}

